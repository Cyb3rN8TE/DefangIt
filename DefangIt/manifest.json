{
        "manifest_version": 2,
    
        "name": "DefangIt",
        "description": "Defangs any selected IPs, URLs and Domains and copies them to your clipboard",
        "version": "1.0.0",
    
        "permissions": [
          "contextMenus",
          "tabs",
          "notifications"
    
          ],
        "icons": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        },
    
        "background": {
          "scripts": ["worker.js"]
        }
}